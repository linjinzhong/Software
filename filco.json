{
    "title": "filco",
    "rules":
    [
        {
            "description": "left_option + E/S/D/F/A/G -> Arrows and head/tail",
            "manipulators":
            [
                {
                    "type": "basic",
                    "from":
                    {
                        "key_code": "s",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_option"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "left_arrow"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from":
                    {
                        "key_code": "d",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_option"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "down_arrow"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from":
                    {
                        "key_code": "e",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_option"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "up_arrow"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from":
                    {
                        "key_code": "f",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_option"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "right_arrow"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from":
                    {
                        "key_code": "a",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_option"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "left_arrow",
                            "modifiers":
                            [
                                "left_command"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from":
                    {
                        "key_code": "g",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_option"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "right_arrow",
                            "modifiers":
                            [
                                "left_command"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "left_option + tab -> left_command + tab",
            "manipulators":
            [
                {
                    "type": "basic",
                    "from":
                    {
                        "key_code": "tab",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_option"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "tab",
                            "modifiers":
                            [
                                "left_command"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "mouse 3（鼠标滚轮）-> desktop switcher",
            "manipulators":
            [
                {
                    "type": "basic",
                    "from":
                    {
                        "pointing_button": "button3"
                    },
                    "to":
                    [
                        {
                            "repeat": false,
                            "key_code": "up_arrow",
                            "modifiers":
                            [
                                "left_control"
                            ],
                            "lazy": false
                        }
                    ]
                }
            ]
        },
        {
            "description": "mouse 4/5（鼠标侧键） -> switch left/right",
            "manipulators":
            [
                {
                    "type": "basic",
                    "from":
                    {
                        "pointing_button": "button4"
                    },
                    "to":
                    [
                        {
                            "key_code": "left_arrow",
                            "modifiers":
                            [
                                "left_control"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from":
                    {
                        "pointing_button": "button5"
                    },
                    "to":
                    [
                        {
                            "key_code": "right_arrow",
                            "modifiers":
                            [
                                "left_control"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "Weterm/Terminal: cmd + R -> ctrl + R",
            "manipulators":
            [
                {
                    "type": "basic",
                    "conditions":
                    [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers":
                            [
                                "^com\\.apple\\.Terminal$",
                                "^com\\.tencent\\.weterm$"
                            ]
                        }
                    ],
                    "from":
                    {
                        "key_code": "r",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_command"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "r",
                            "modifiers":
                            [
                                "left_control"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "Weterm/Terminal: cmd + C -> ctrl + C",
            "manipulators":
            [
                {
                    "type": "basic",
                    "conditions":
                    [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers":
                            [
                                "^com\\.apple\\.Terminal$",
                                "^com\\.tencent\\.weterm$"
                            ]
                        }
                    ],
                    "from":
                    {
                        "key_code": "c",
                        "modifiers":
                        {
                            "mandatory":
                            [
                                "left_command"
                            ],
                            "optional":
                            [
                                "any"
                            ]
                        }
                    },
                    "to":
                    [
                        {
                            "key_code": "c",
                            "modifiers":
                            [
                                "left_control"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}